int consumir(FILE *con){

	char nomcon[20];
		
	
		
		while(!feof(con)){
			
			fscanf(con,"%s\n",nomcon);
			printf("Se consumi√≥: %s\n",nomcon);
			

		}
		
	

	return 0;
}
